{
  "manifest_version": 3,
  "name": "Quote Scraper",
  "version": "1.0",
  "description": "Scrape quotes from a website.",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
